#!/bin/sh

wget <%= node['service']['MyServletWAR']['URL']%> --append-output=<%= node['service']['MyServletWAR']['logFile']%>
rm -f <%= node['service']['MyServletWAR']['webappsPath']%>/<%= node['service']['MyServletWAR']['WarFile']%>
cp <%= node['service']['MyServletWAR']['WarFile']%> <%= node['service']['MyServletWAR']['webappsPath']%>

# temporary command to list
touch | date +%F__%k%M
